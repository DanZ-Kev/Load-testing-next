{
  "name": "load-testing-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade load testing platform with stunning Apple Liquid Glass UI",
  "private": true,
  "keywords": [
    "load-testing",
    "performance-testing",
    "stress-testing",
    "monitoring",
    "enterprise",
    "liquid-glass",
    "real-time",
    "nextjs"
  ],
